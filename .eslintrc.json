{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": ["airbnb-base"],
  "rules": {
    "no-shadow": "off",
    "no-param-reassign": "off",
    "eol-last": "off",
    "max-classes-per-file": "off",
    "no-use-before-define": "off",
    "max-len": "off",
    "class-methods-use-this": "off",
    "no-restricted-syntax": "off",
    " no-unused-vars": "off",
    "prefer-const": "off",
    "no-unused-vars": "off",
    "no-useless-escape": "off",
    "no-nested-ternary": "off",
    "no-continue": "off",
    "no-underscore-dangle": "off",
    "no-loop-func": "off",
    "default-case": "off",
    "prefer-rest-params": "off",
    "guard-for-in": "off",
    "no-restricted-globals": "off",
    "brace-style": "off",
    "import/extensions": [
      1,
      {
        "js": "always",
        "json": "always"
      }
    ]
  },
  "ignorePatterns": ["dist/", "build/"]
}
